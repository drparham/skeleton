{
    "name": "pta/skeleton",
    "description": "Description of Package",
    "keywords": [
        "pta",
        "skeleton"
    ],
    "homepage": "https://github.com/drparham/skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Parham",
            "email": "dr.parham@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "~5.2",
    },
    "require-dev": {
        "orchestra/testbench": "~3.2",
        "friendsofphp/php-cs-fixer": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/migrations",
            "database/seeders"
        ],
        "psr-4": {
            "Pta\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pta\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
